{
  "name": "yag-id-hash",
  "description": "A reversible base62 ID obfuscator",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "private": true,
  "homepage": "https://github.com/yasselavila/id-hash",
  "author": {
    "name": "Yassel Avila Gil",
    "url": "http://yasselavila.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yasselavila/id-hash.git"
  },
  "keyworks": [
    "yag",
    "id",
    "hash",
    "obfuscator",
    "id-hash",
    "id-obfuscator",
    "base62",
    "base-62"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "start": "npm run build:w",
    "clean": "rm -rf ./dist && mkdir dist",
    "dist:data": "yag-pkg-dist && cp ./README.md ./dist/ && cp ./LICENSE.txt ./dist/",
    "build:pre": "npm run clean && npm run dist:data",
    "build": "npm run build:pre && npm run tsc",
    "build:w": "npm run build:pre && npm run tsc:w",
    "tsc": "tsc --outDir ./dist",
    "tsc:w": "npm run tsc -- -w",
    "test": "mocha -r ts-node/register src/tests/**/*.ts"
  },
  "devDependencies": {
    "typescript": "^2.1.0",
    "ts-node": "^2.1.0",
    "mocha": "^3.2.0",
    "chai": "^3.5.0",
    "@types/mocha": "^2.2.39",
    "@types/chai": "^3.4.34"
  }
}
